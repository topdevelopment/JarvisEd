<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false">
        <shape android:shape="oval">
            <stroke android:color="@color/jarvisSecondaryDarkColor" android:width="@dimen/ui_stroke_width"/>
            <solid android:color="@color/jarvisSecondaryColor"/>
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="@color/jarvisSecondaryDarkColor"/>
        </shape>
    </item>

</selector>